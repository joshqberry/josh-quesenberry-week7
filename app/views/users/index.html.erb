<h1>All users</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>User name&nbsp;&nbsp;&nbsp;</th>
      <th>About</th>
      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>



    <% @users.each do |user| %>
      <tr>
        <td><%= user.user_name %></td>
        <td><%= user.about %></td>
      </tr>

</tbody>

<% end %>
